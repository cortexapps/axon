{
  "private": [
    {
      "method": "any",
      "path": "/*",
      "origin": "${TEST_API}",
      "headers": {
        "x-private-key": "${PRIVATE_KEY}",
        "x-custom-header": "test-value"
      },
      "auth": {
        "scheme": "bearer",
        "token": "${TEST_TOKEN}"
      }
    },
    {
      "method": "GET",
      "path": "/api/v1/*",
      "origin": "${ANOTHER_API}",
      "headers": {
        "x-api-key": "${API_KEY}",
        "x-version": "v1"
      }
    }
  ]
}