{
  "private": [
    {
      "method": "any",
      "path": "/*",
      "origin": "${GITHUB_API}",
      "auth": {
        "scheme": "bearer",
        "token": "${GITHUB_TOKEN}"
      }
    },
    {
      "method": "POST",
      "path": "/graphql",
      "origin": "${GITHUB_GRAPHQL}",
      "auth": {
        "scheme": "bearer",
        "token": "${GITHUB_TOKEN}"
      }
    }
  ]
}
